  <parent>
    <groupId>com.github.xgp</groupId>
    <artifactId>sql2java</artifactId>
    <version>0.9.1-SNAPSHOT</version>
  </parent>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.3.2</version>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
